{
  "expo": {
    "name": "Attendify",
    "slug": "attendify",
    "description": "Application to check attendances on live events",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/attendify-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/attendify-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.xavierloos.attendify"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.xavierloos.attendify"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": ""
      }
    },
    "owner": "attendify_infosys"
  }
}
