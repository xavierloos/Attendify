{
  "expo": {
    "name": "Attendify",
    "slug": "attendify",
    "description":"Application to check attendances on live events",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/attendify-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/attendify-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a4c15d67-2421-44d0-aa01-ff9f46824e67"
      }
    },
    "owner": "xavierloos"
  }
}
